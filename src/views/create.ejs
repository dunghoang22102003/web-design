<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Create users</title>
    <link rel="stylesheet" href="/css/create.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css"
        integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
</head>

<body id="body">
    <div class="navication">
        <a class="navication-content " href="/">Home</a>
        <a class="navication-content display-color" href="/create">Create user</a>
    </div>

    <div class="form">
        <form action="/create-user" method="post">
            <legend class="title">Form add new users</legend>
            <div class="input-group">
                <label class="type-name-1" id="email-id">Email: </label>
                <input name="email" class="type" type="text" />
            </div>
            <div class="input-group">
                <label class="type-name-2" id="name-id">Name: </label>
                <input name="name" class="type" type="text" />
            </div>
            <div class="input-group">
                <label class="type-name-3" id="city-id">City: </label>
                <input name="city" class="type" type="text" />
            </div>
            <div class="action">
                <button class="button">Save</button>
            </div>
        </form>
    </div>





</body>

</html>